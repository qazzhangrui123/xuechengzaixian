<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuecheng.work.mapper.WorkMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xuecheng.work.model.po.Work">
        <id column="id" property="id" />
        <result column="company_id" property="companyId" />
        <result column="company_name" property="companyName" />
        <result column="bind_course" property="bindCourse" />
        <result column="create_date" property="createDate" />
        <result column="change_date" property="changeDate" />
        <result column="question" property="question" />
        <result column="status" property="status" />
        <result column="titile" property="titile" />
        <result column="username" property="username" />
        <result column="user_num" property="userNum" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, company_id, company_name, bind_course, create_date, change_date, question, status, titile, username, user_num
    </sql>

</mapper>
